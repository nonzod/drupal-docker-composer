# Ignore directories generated by Composer
drush/contrib
vendor
bin
web/core
web/libraries
web/modules/contrib
web/themes/contrib
web/profiles/contrib
.env.local

# Ignore configuration files that may contain sensitive information.
web/sites/*/settings*.php
web/sites/*/services*.yml

# Ignore Behat screenshots directory
features/screenshots
!features/screenshots/.gitkeep

# Ignore Drupal's file directory
private/*
tmp/*
web/sites/*/files
web/sites/*/private

# Ignore SimpleTest multi-site environment.
web/sites/simpletest

# Ignore files generated by PhpStorm
.idea

# Ignore files generated by VSCode
.vscode

# Ignore apple stuff
.DS_Store

# Ignore files generated by building process
exit_code

# General exclusions.
/.cache/composer
!/.cache/.gitkeep
*.txt
!/.bmeme/config/drupal/*/robots.txt
*.bz2
*.gz
*.zip
*.sql
*.dmp
tmp*
*.retry

# Optional exclusions.
node_modules
bower_components

# Ignore assets in res theme
web/themes/custom/res/assets
